{
  "name": "insert-tab-when-press-tab",
  "displayName": "Insert Tab when press Tab",
  "description": "Limit the 'Insert Spaces' config to only the indent, and enable you insert Tab when press Tab elsewhere..",
  "version": "0.0.1",
	"publisher": "LinaLight",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/LinaLight/Insert-Tab-when-press-Tab.git"
	},
	"keywords": [
		"vscode",
		"tab",
		"indent",
		"space",
		"align"
	],
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "icon": "icon.png",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
				"command": "Insert Tab",
        "title": "Insert Tab"
      }
    ],
		"keybindings": [
      {
				"command": "Insert Tab",
				"key": "Tab",
				"when": "editorTextFocus && !editorReadOnly && !editorTabMovesFocus && !suggestWidgetVisible && !inSnippetMode && config.editor.tabCompletion=='on'"
			}
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
